{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "types": ["vile/client"],
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "config/configValues.ts"],
  "compilerOptions": {
    "esModuleInterop": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@car-brands/*": ["./public/car-brands/*"],
      "@icons/*": ["./public/icons/*"],
      "@config/*": ["./config/*"],
      "@helpers/*": ["./src/vue-helpers/*"]
    }
  }
}

{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "types": ["vite/client"],
    "baseUrl": ".",
    "module": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./src/*"],
      "@car-brands/*": ["./public/car-brands/*"],
      "@icons/*": ["./public/icons/*"],
      "@config/*": ["./config/*"],
      "@helpers/*": ["./src/vue-helpers/*"]
    }
  }
}

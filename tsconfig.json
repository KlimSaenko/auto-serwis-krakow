{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "target": "ES2020",
    "module": "Node16",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@car-brands/*": ["./public/car-brands/*"],
      "@icons/*": ["./public/icons/*"],
      "@config/*": ["./config/*"],
      "@helpers/*": ["./src/vue-helpers/*"]
    }
  },
  "exclude": ["node_modules"]
}
